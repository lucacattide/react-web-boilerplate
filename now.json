{
  "public": false,
  "name": "",
  "version": 2,
  "routes": [{
    "src": "/robots.txt",
    "dest": "https:///robots.txt"
  }, {
    "src": "/sitemap.xml",
    "dest": "https:///sitemap.xml"
  }, {
    "src": "^/service-worker.js$",
    "dest": "/_next/static/service-worker.js",
    "headers": {
      "cache-control": "public, max-age=43200, immutable",
      "Service-Worker-Allowed": "/"
    }
  }],
  "build": {
    "env": {
      "RECAPTCHA_SECRET_KEY": "@recaptcha",
      "EMAIL_SERVICE": "@email_service",
      "EMAIL_TEMPLATE_COOKIES": "@email_template_cookies",
      "EMAIL_USER": "@email_user",
      "GTM_AUTH_PROD": "@gtm_auth_blog"
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": false
  }
}
